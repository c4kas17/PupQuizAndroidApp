<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pupquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScorePage" />
        <activity android:name=".SubmitPageTrivia" />
        <activity android:name=".SubmitPageBreed" />
        <activity android:name=".QuizPageTrivia" />
        <activity android:name=".QuizPageBreed" />
        <activity android:name=".InstructionsTrivia" />
        <activity android:name=".InstructionsGuessTheBreed" />
        <activity android:name=".TypeOfQuizPage" />
        <activity android:name=".MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>